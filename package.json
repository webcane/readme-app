{
  "name": "svelte-readme-app",
  "version": "1.0.0",
  "description": "webapp holds links to articles to read",
  "scripts": {
    "build": "webpack:build",
    "start": "serve",
    "rollup:build": "del-cli src/main/resources/public/build && rollup --config",
    "rollup:start": "del-cli src/main/resources/public/build && rollup --config --watch",
    "webpack:build": "del-cli src/main/resources/public/build && cross-env NODE_ENV=production webpack",
    "webpack:start": "del-cli src/main/resources/public/build && webpack-dev-server",
    "serve": "sirv src/main/resources/public",
    "serve:dev": "sirv src/main/resources/public --single --dev"
  },
  "keywords": [
    "svelte"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/webcane/readme-app.git"
  },
  "author": "Mikhail Niedre",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "del-cli": "^3.0.1",
    "html-webpack-plugin": "^4.2.0",
    "mini-css-extract-plugin": "^0.11.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.28.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "serialize-javascript": ">=5.0.1",
    "svelte": "^3.26.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "serve": "^11.3.2",
    "style-loader": "^1.2.1"
  },
  "dependencies": {
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "sirv-cli": "^1.0.6",
    "svelte-tags-input": "^2.6.5",
    "sveltestrap": "^3.8.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  }
}
