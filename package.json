{
  "name": "svelte-readme-app",
  "version": "1.0.0",
  "description": "webapp holds links to articles to read",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv src/main/resources/public",
    "start:dev": "sirv src/main/resources/public --single --dev",
    "build:java": "del-cli src/main/resources/public/build && rollup -c"
  },
  "keywords": [
    "svelte"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/webcane/readme-app.git"
  },
  "author": "Mikhail Niedre",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "del-cli": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.26.11",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "serialize-javascript": ">=5.0.1",
    "svelte": "^3.25.0"
  },
  "dependencies": {
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "sirv-cli": "^1.0.6",
    "svelte-tags-input": "^2.6.5",
    "sveltestrap": "^3.8.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  }
}
