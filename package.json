{
  "name": "svelte-readme-app",
  "version": "1.0.0",
  "description": "webapp holds links to articles to read",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv src/main/resources/public",
    "start:dev": "sirv src/main/resources/public --single --dev",
    "build:java": "rm ./src/main/resources/public/build/bundle.js.map && rm ./src/main/resources/public/build/bundle.js && rm ./src/main/resources/public/build/bundle.css.map && rm ./src/main/resources/public/build/bundle.css && rollup -c"
  },
  "keywords": [
    "svelte"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sveltejs/svelte-todomvc.git"
  },
  "author": "Mikhail Niedre",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-filesize": "^6.0.1",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4"
  }
}
